<script id="Consoloid-Ui-List-View-Base" type="text/x-jqote-template">
<![CDATA[
  <div class="list-wrapper" style="overflow: hidden"></div>
]]>
</script>

<script id="Consoloid-Ui-List-Widget" type="text/x-jqote-template">
<![CDATA[
  <div class="list-widget">
    <div class="list-view"></div>
  </div>
]]>
</script>

<script id="Consoloid-Ui-List-BaseFilterWidget" type="text/x-jqote-template">
<![CDATA[
  <div class="list-filter">
    <div class="clear-filters"><img alt="remove" src="/Consoloid/Ui/images/trash.png" /></div>
    <div><%= __("Currently filtering for:") %> <div class="filter-value"></div></div>
  </div>
]]>
</script>

<script id="Consoloid-Ui-List-Scrollbar-Base" type="text/x-jqote-template">
<![CDATA[
  <div class="scrollbar-wrapper">
    <button class="scrollbar-button-first-page">▲ ▲</button>
    <button class="scrollbar-button-previous-page">▲</button>
    <span class="scrollbar-pagenumber"><%= Math.round(this.pageNumber) + 1 %></span>
    <button class="scrollbar-button-next-page">▼</button>
    <button class="scrollbar-button-last-page">▼ ▼</button>
  </div>
]]>
</script>

<script id="Consoloid-Ui-List-Dialog" type="text/x-jqote-template">
<![CDATA[
  <div class="list-dialog"></div>
]]>
</script>

<script id="Consoloid-Ui-List-View-Throbber" type="text/x-jqote-template">
<![CDATA[
  <div class="throbber-table">
    <div class="throbber-cell">
      <img class="throbber" alt="throbber" src="/Consoloid/Ui/images/throbber.gif" />
    </div>
  </div>
]]>
</script>

<script id="Consoloid-Ui-List-View-Empty" type="text/x-jqote-template">
<![CDATA[
  <p class='empty-message'><%= this %></p>
]]>
</script>
